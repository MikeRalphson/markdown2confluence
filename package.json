{
  "name": "markdown2confluence",
  "version": "0.0.1",
  "description": "Convert Markdown to Confluence markup",
  "main": "index.js",
  "scripts": {
    "test": "node test.js",
    "build": "npm run build-html && npm run build-js",
    "build-html": "jade -p node_modules/jade-gist/. < browser/index.jade > browser/index.html",
    "build-js": "webpack browser/app.js --output-filename=browser/bundle.js",
    "readme": "pretty-readme > readme.md"
  },
  "author": "Milen Georgiev <Shogo>",
  "license": "ISC",
  "dependencies": {
    "jade": "^1.11.0",
    "jade-gist": "^1.0.5",
    "marked": "^1.2.0",
    "min-qs": "^1.3.0",
    "min-util": "^3.6.1",
    "raw-loader": "^4.0.2",
    "webpack": "^5.1.0"
  },
  "bin": {
    "markdown2confluence": "bin/markdown2confluence.js"
  },
  "devDependencies": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/Shogobg/markdown2confluence.git"
  },
  "keywords": [
    "markdown",
    "confluence",
    "markup",
    "convert"
  ],
  "bugs": {
    "url": "https://github.com/Shogobg/markdown2confluence/issues"
  },
  "homepage": "https://shogo.eu/"
}
